?ggbarmaker
# Between subject design.
x <- rnorm(100,100,15)
y <- sample(c("Group 1","Group 2"),100,TRUE)
z <- sample(c("Low","Med","High"),100,TRUE)
# Only one factor.
ggbarmaker(x,y)
# Two factors.
ggbar.maker(x,y,z)
# Between subject design.
x <- rnorm(100,100,15)
y <- sample(c("Group 1","Group 2"),100,TRUE)
z <- sample(c("Low","Med","High"),100,TRUE)
# Only one factor.
ggbarmaker(x,y)
# Two factors.
ggbarmaker(x,y,z)
roxygen2::roxygenize()
devtools::document()
